<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="586.0" prefWidth="673.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SlideShowController">
   <children>
      <Pane prefHeight="500.0" prefWidth="706.0">
         <children>
            <Button fx:id="btnPrevious" layoutX="21.0" layoutY="12.0" mnemonicParsing="false" onAction="#onClickPrevious" text="Previous">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="btnNext" layoutX="511.0" layoutY="12.0" mnemonicParsing="false" onAction="#onClickNext" text="Next">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <ScrollPane layoutX="-3.0" layoutY="37.0">
               <content>
                  <ImageView fx:id="imageViewer" fitHeight="545.0" fitWidth="707.0" pickOnBounds="true" preserveRatio="true" />
               </content>
            </ScrollPane>
            <Label layoutX="246.0" layoutY="11.0" text="SlideShow">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <Button fx:id="btnBackToPhotos" layoutX="393.0" layoutY="12.0" mnemonicParsing="false" onAction="#OnClickBackToPhotos" text="Back to Photos">
               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </children>
</AnchorPane>
